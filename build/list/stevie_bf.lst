0061 6C4A CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 2
0062 6C4C CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 3
0063 6C4E CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 4
0064 6C50 CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 5
