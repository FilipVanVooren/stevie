0061 6C4E CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 2
0062 6C50 CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 3
0063 6C52 CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 4
0064 6C54 CD74  46         mov   *tmp0+,*tmp1+         ; Copy word 5
